<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorBeige"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/new_user"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.5"
        android:layout_marginTop="2dp"
        android:background="@color/colorBlue"
        android:padding="20dp"
        >

        <ImageView
            android:id="@+id/new_user_icon"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:src="@drawable/add_user_icon"
            android:layout_centerVertical="true"
            android:tint="@color/colorWhite"/>
        <ImageView
            android:id="@+id/new_user_arrow"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:src="@drawable/icon_right_arrow"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:tint="@color/colorWhite"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:paddingLeft="20dp"
            android:paddingRight="10dp"
            android:layout_toRightOf="@id/new_user_icon"
            android:layout_toLeftOf="@id/new_user_arrow"
            android:text="Registrer dit barn hos abdo."
            android:textSize="24dp"
            android:textColor="@color/colorWhite"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/new_user_code"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.5"
        android:layout_marginTop="2dp"
        android:background="@color/colorPurple"
        android:padding="20dp"
        >

        <ImageView
            android:id="@+id/new_user_code_icon"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:src="@drawable/icon_sharecode"
            android:layout_centerVertical="true"
            android:tint="@color/colorWhite"/>
        <ImageView
            android:id="@+id/new_user_code_arrow"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:src="@drawable/icon_right_arrow"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:tint="@color/colorWhite"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:paddingLeft="20dp"
            android:paddingRight="10dp"
            android:layout_toRightOf="@id/new_user_code_icon"
            android:layout_toLeftOf="@id/new_user_code_arrow"
            android:text="FÃ¥ adgang til et registreret barn, ved at bruge en delekode."
            android:textSize="24dp"
            android:textColor="@color/colorWhite"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/new_user_email"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.5"
        android:layout_marginTop="2dp"
        android:layout_marginBottom="2dp"
        android:background="@color/colorGreen"
        android:padding="20dp"
        >

        <ImageView
            android:id="@+id/new_user_email_icon"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:src="@drawable/icon_email"
            android:layout_centerVertical="true"
            android:tint="@color/colorWhite"/>
        <ImageView
            android:id="@+id/new_user_email_arrow"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:src="@drawable/icon_right_arrow"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:tint="@color/colorWhite"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:paddingLeft="20dp"
            android:paddingRight="10dp"
            android:layout_toRightOf="@id/new_user_email_icon"
            android:layout_toLeftOf="@id/new_user_email_arrow"
            android:text="Gendan din tidligere bruger via. email."
            android:textSize="24dp"
            android:textColor="@color/colorWhite"/>
    </RelativeLayout>



