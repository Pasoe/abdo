<ScrollView
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent">
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="@color/colorGreen"
            android:padding="20dp"
            >
            <ImageView
                android:id="@+id/stamdata_header_image"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/add_user_icon"
                android:layout_centerVertical="true"
                android:tint="@color/colorWhite"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                android:layout_toRightOf="@id/stamdata_header_image"
                android:text="Registrer dit barn i appen."
                android:textSize="24dp"
                android:textColor="@color/colorWhite"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/colorBlue"
            android:id="@+id/create_child_data_category">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:textColor="@color/colorWhite"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="15dp"
                android:text="Stamdata"/>
            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/icon_right_arrow"
                android:tint="@color/colorWhite"
                />
        </RelativeLayout>

        <android.support.v7.widget.RecyclerView
            android:id="@+id/stamdata_list"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_marginBottom="10dp"
            android:overScrollMode="never"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/colorBlue"
            android:id="@+id/create_child_medicine_category">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:textColor="@color/colorWhite"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="15dp"
                android:text="Medicin"/>
            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/icon_right_arrow"
                android:tint="@color/colorWhite"
                />
        </RelativeLayout>

        <android.support.v7.widget.RecyclerView
            android:id="@+id/medicine_list"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_marginBottom="10dp"
            android:overScrollMode="never"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/colorBlue"
            android:id="@+id/create_child_allergy_category">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:textColor="@color/colorWhite"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="15dp"
                android:text="Allergier"/>
            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/icon_right_arrow"
                android:tint="@color/colorWhite"
                />
        </RelativeLayout>

        <android.support.v7.widget.RecyclerView
            android:id="@+id/allergies_list"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_marginBottom="10dp"
            android:overScrollMode="never"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/colorBlue"
            android:id="@+id/create_child_supplement_category">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:textColor="@color/colorWhite"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="15dp"
                android:text="Kosttilskud"/>
            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/icon_right_arrow"
                android:tint="@color/colorWhite"
                />
        </RelativeLayout>

        <android.support.v7.widget.RecyclerView
            android:id="@+id/supplements_list"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:overScrollMode="never"
            />
    </LinearLayout>
</ScrollView>